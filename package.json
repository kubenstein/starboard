{
  "name": "starboard",
  "author": "Jakub Niewczas",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "v7.2.0"
  },
  "scripts": {
    "dev": "node ./dev-console.js",
    "dev:frontend": "webpack-dev-server",
    "dev:backend": "MONGODB_URI='localhost/db' nodemon --watch backend/ backend/server.js",
    "dev:db": "mongod --timeStampFormat=ctime -v --dbpath=./db/development --port 27017",
    "dev:linter": "eslint -c .eslintrc \"./*.js\" \"./backend/**\" \"./frontend/**\" \"./spec/**\" || true",
    "dev:linter:ci": "nodemon -e jsx,js --ignore build/ --watch . --exec 'npm run dev:linter'",
    "build": "webpack -p",
    "postinstall": "npm run build",
    "start": "node backend/server.js"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "form-serialize": "^0.7.1",
    "node-sass": "^4.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "uuid": "^3.0.1",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.7.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mongodb": "^2.2.26",
    "mongodb-prebuilt": "^6.3.1",
    "mongojs": "^2.4.0",
    "nodemon": "^1.11.0",
    "webpack-dev-server": "^1.16.2"
  }
}
