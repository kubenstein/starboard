{
  "name": "starboard",
  "author": "Jakub Niewczas",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "engines": {
    "node": ">=4.3.2"
  },
  "bin": {
    "starboard": "bin/starboard"
  },
  "main": "dist/starboard.js",
  "scripts": {
    "dev": "node ./src/scripts/dev-console.js",
    "dev:frontend": "webpack-dev-server --config ./src/webpack-frontend.config.js",
    "dev:backend:es6compiler": "webpack --watch --config ./src/webpack-lib.config.js",
    "dev:backend:server": "REPO_URL=$(pwd)/.tmp/fakeDevelopmentRemoteGitRepo/ DEBUG=true nodemon --inspect --debug --watch .tmp/backend/ .tmp/backend/bundled-server-runner.js",
    "dev:linter": "eslint -c .eslintrc \"./src/*.js\" \"./src/backend/**\" \"./src/frontend/**\" \"./src/spec/**\" \"./src/lib/**\" || true",
    "dev:linter:ci": "nodemon -e jsx,js --watch ./src/ --exec 'npm run dev:linter'",
    "dev:db:create": "node ./src/scripts/db-create-development.js",
    "dev:db:clear": "./src/scripts/wipe-development-db.sh",
    "dev:localRelease": "./src/scripts/local-release.sh",
    "build": "rm -rf ./dist && npm run build:lib && npm run build:frontend",
    "build:frontend": "NODE_ENV=production webpack -p --config ./src/webpack-frontend.config.js",
    "build:lib": "NODE_ENV=production webpack --config ./src/webpack-lib.config.js"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.2",
    "mkdirp": "^0.5.1",
    "multer": "^1.3.0",
    "socket.io": "^2.0.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "axios": "^0.16.1",
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.7.5",
    "css-loader": "^0.26.1",
    "dragula": "^3.7.2",
    "es6-promise": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "form-serialize": "^0.7.1",
    "js-cookie": "^2.1.4",
    "node-sass": "^4.0.0",
    "nodemon": "^1.11.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "sass-loader": "^4.1.0",
    "socket.io-client": "^2.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
